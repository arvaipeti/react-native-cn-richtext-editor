{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 9
    },
    "env": {
        "es6": true
    },
    "rules": {
        "comma-dangle": [
            "error",
            "never"
        ],
        "indent": [
            "error",
            4
        ],
        "indent-legacy": "off",
        "no-underscore-dangle": [
            "error",
            {
                "allow": [
                    "_resolve"
                ]
            }
        ]
    }
}
